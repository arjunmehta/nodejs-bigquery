{
  "name": "discovery-tsd",
  "type": "module",
  "description": "Generates TypeScript declaration files from Google's Discovery API",
  "version": "0.3.0",
  "license": "ISC",
  "author": "Dave Gramlich <callmehiphop@gmail.com>",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": "callmehiphop/discovery-tsd",
  "main": "src/index.js",
  "files": [
    "src",
    "README.md"
  ],
  "bin": {
    "dtsd": "src/cli.js"
  },
  "keywords": [
    "google",
    "discovery",
    "api",
    "types",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint ./{src,test}/*.js",
    "prettier": "prettier --write ./{src,test}/*.js",
    "compile": "tsc -p . && cp ./src/templates/*.tmpl ./build/src/templates/ && cp ./test/fixtures/**/*.{json,d.ts} ./build/test/fixtures/",
    "test": "c8 mocha build/test"
  },
  "dependencies": {
    "got": "^13.0.0",
    "lodash.isempty": "^4.4.0",
    "lodash.template": "^4.4.0",
    "meow": "^12.1.1",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@google-cloud/common": "^5.0.0",
    "@types/got": "^9.6.12",
    "assert": "^2.1.0",
    "c8": "^8.0.1",
    "eslint": "^5.15.3",
    "gts": "^5.2.0",
    "mocha": "^10.2.0",
    "requirejs": "^2.3.6",
    "typescript": "^5.2.2"
  }
}
